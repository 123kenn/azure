                        "caching": "ReadWrite",
                        "managedDisk": {
                            "storageAccountType": "Premium_LRS",
                            "id": "[resourceId('Microsoft.Compute/disks', concat(parameters('virtualMachines_armtemplatedeploymentvm_name'), '_disk1_ac457010e1c648da944a905f0da50c53'))]"
